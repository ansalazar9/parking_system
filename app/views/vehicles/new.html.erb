<h2>Registro de vehiculos</h2>

<%= form_for @vehicle, url: {action: 'create'} do |form| %>

  <div class="field">
    <%= form.label :model%><br />
    <%= form.text_field :model, autofocus: true %>
  </div>

  <div class="field">
    <%= form.label :vin%><br />
    <%= form.text_field :vin, autofocus: true %>
  </div>

  <div class="field">
    <%= form.label :year%><br />
    <%= form.date_field :year, autofocus: true %>
  </div>
  
  <div class="field">
    <%= form.submit :Guardar%><br />
  </div>

<% end %>
